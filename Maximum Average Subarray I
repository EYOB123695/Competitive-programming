class Solution:
    def findMaxAverage(self, nums: List[int], k: int) -> float:
        ans = -float("inf")
        curr = sum(nums[:k])
        ans = curr
        i = 0
        j =  k
        while j < len(nums):
            curr += nums[j]
            curr -= nums[i]
            ans= max(curr,ans)
            j+= 1
            i += 1 
        return (ans / k)







        
